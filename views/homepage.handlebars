<ol class="project-list">
    {{!-- projects array --}}
    {{#each projects as |project|}}
        <ul>
            {{> project-info project}}
        </ul>
    {{/each}}
</ol>

<div class="login">
    <div class="row justify-content-center">
        <div class="col-lg-7" style="margin-top: 50px;">
            <div class="card">
                <h2 class="card-title">Please log in to your account</h2>
                <input class="" id="username" type="text">
                <input class="" id="password" type="text">
                <button class="" id="loginbtn" type="submit">
                    <span class="">Log in</span>
                </button>
            </div>
        </div>
    </div>
</div>